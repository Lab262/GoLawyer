<?xml version="1.0" encoding="utf-8"?>
<mobigap.golawyer.LawyerServiceFollowing.LawyerServiceStatusChat.LawyerServiceStatusChatActionButtonsView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/headerRegister"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content"
    android:padding="10dp"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="horizontal"
        android:weightSum="100">


        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/confirmServiceStatusChatButton"
            android:layout_weight="50"
            android:background="@android:color/transparent"
            android:scaleType="fitXY"
            android:src="@drawable/ic_button_confirm" />

        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/cancelServiceStatusChatButton"
            android:layout_weight="50"
            android:background="@android:color/transparent"
            android:scaleType="fitXY"
            android:src="@drawable/button_cancel" />
    </LinearLayout>
</mobigap.golawyer.LawyerServiceFollowing.LawyerServiceStatusChat.LawyerServiceStatusChatActionButtonsView>
